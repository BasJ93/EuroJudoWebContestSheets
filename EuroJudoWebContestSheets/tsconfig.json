{
  "compilerOptions": {
    "baseUrl": ".",
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es5",
    "allowJs": true,
    "outDir": "ClientApp/js",
    "jsx": "react"
  },
  "files": [
    "ClientApp/contest.ts",
    "ClientApp/ContestOrderList.tsx",
    "ClientApp/ContestOrderApp.tsx",
    "ClientApp/sheet.ts"
  ],
  "exclude": [
    "node_modules",
    "wwwroot",
    "out"
  ],
  "compileOnSave": true,
  "typeAcquisition": {
    "enable": true
  }
}